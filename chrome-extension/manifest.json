{
  "name": "The One Where They Used Comment Sentiment Analysis to Modify and Image",
  "manifest_version": 2,
  "description": "",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "tabs",
    "https://facebook.com/*",
    "https://instagram.com/*",
    "http://localhost:3000/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.instagram/*", "https://facebook.com/*"],
      "js": ["jquery.min.js"]
    }
  ],
  "browser_action": {
    "default_title": "Image Feels",
    "default_popup": "main.html"
  }
}
